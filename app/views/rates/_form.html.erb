<%= simple_form_for(@rate) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :startup %>
    <%= f.input :name %>
    <%= f.input :website %>
    <%= f.input :video %>
    <%= f.input :logo %>
    <%= f.input :text %>
    <%= f.input :idea %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
