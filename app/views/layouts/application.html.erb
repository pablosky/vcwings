    <h5>ABOUT</h5>

    </div>  







     <% end %>    

     <div class="span1">
    </div>  


   </div>
   
</div>







</header>  







<header>
<center>
<br>

        <ul class="breadcrumb" >

              <% if signed_in? %>
              <li>&nbsp;<%= link_to "Dashboard ", dashboard_path %><span class="divider">/</span></li> 

              <li>&nbsp;&nbsp;<%= link_to "My Profile  ",  :controller => :profiles , :action => :myprofile , :id=>current_user.id %><span class="divider">/</span></li>

             
              <li>&nbsp;<%= link_to " My Startups ", :controller => :startups , :action => :mystartups , :id=> current_user.id %><span class="divider">/</span></li>
             

              <li>&nbsp;&nbsp;<%= link_to " Create Startup  ", new_startup_path(@startup)   %><span class="divider">/</span></li>
              
