              <li>&nbsp;<%= link_to "Sign out   ", sign_out_path, :method => :delete %> <span class="divider">/</span></li>
              
             
             
               
          <% else %>
              <li>&nbsp;<%= link_to "Login ", sign_in_path %><span class="divider">/</span></li>

              <li>&nbsp;<%= link_to " Join   ", sign_up_path %> <span class="divider">/</span></li>

               <li>&nbsp;<%= link_to " Face " ,"http://www.facebook.com"  %> <span class="divider">/</span></li>
          <% end %>         





              <li> </li>




      </ul>
   
  




 






</center>
</header> <!-- fin del header -->




 



  
  <%= yield %>
 
  

<%= javascript_include_tag "application" %>
    

