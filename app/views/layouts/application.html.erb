       </div>
  

       <div class="span6">
      <h5>UPLOAD <a href="#">your startup</a></h5>
        </div>


   </div>
  </div>



  <div class="span3">

    <div class="input-append">

      <%= form_tag ( {:controller => "dashboard" , :action => "search"} ) do %>
      <%= text_field_tag(:name)   %>
      
      <button class='btn btn-info' style='width:40px;'>
      <i class="icon-search"></i>
      </button>


      

      <% end %>


    </div>

  </div>
    

    <% if signed_in? %>

     <div class="span3">

      <h5><%= link_to "LOG OUT   ", sign_out_path, :method => :delete %></h5>
     </div>





    <% else %>
    <div class="span1">
    <h5><%= link_to "LOGIN", sign_in_path %></h5></div>  


   <div class="span1">
   <h5><%= link_to " JOIN   ", sign_up_path %> <span class="divider"></h5></div>  
 
