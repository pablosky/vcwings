



<%- model_class = Profile -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fullname) %>:</strong></dt>
  <dd><%= @profile.fullname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @profile.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:country) %>:</strong></dt>
  <dd><%= @profile.country %></dd>
  <dt><strong><%= model_class.human_attribute_name(:biography) %>:</strong></dt>
  <dd><%= @profile.biography %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @profile.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar) %>:</strong></dt>
  <dd><%= @profile.avatar %></dd>
  <dt><strong><%= model_class.human_attribute_name(:wallet) %>:</strong></dt>
  <dd><%= @profile.wallet %></dd>
  <dt><strong><%= model_class.human_attribute_name(:money) %>:</strong></dt>
  <dd><%= @profile.money %></dd>
</dl>